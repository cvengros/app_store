          "optional":true
        },
        "status":{
          "source_column":"Status"
        }
      }
    },
    "activity_owner":{
      "source_object":"User",
      "columns":{
        "id":{
          "source_column":"Id"
        },
        "name":{
          "source_column":"Name"
        },
        "url":{
          "source_column_concat":[
