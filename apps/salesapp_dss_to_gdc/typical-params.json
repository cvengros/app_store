        },
        "id":{
          "source_column":"Id"
        },
        "order":{
          "source_column":"SortOrder"
        },
        "name":{
          "source_column":"MasterLabel"
        },
        "is_active":{
          "source_column":"IsActive"
        }
      }
    },
