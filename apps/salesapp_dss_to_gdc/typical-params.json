        "id":{
          "source_column":"Id"
        },
        "name":{
          "source_column":"Name"
        },
        "url":{
          "source_column_concat":[
            ":salesforce_server",
            "Id"
          ]
        },
        "department":{
          "source_column":"Department"
        },
        "region":{
